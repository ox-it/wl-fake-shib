<web-app>
	<display-name>Fake Shibboleth Login</display-name>

	<servlet>
		<servlet-name>login.jsp</servlet-name>
		<jsp-file>/login.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>login.jsp</servlet-name>
		<url-pattern>/LoginTwoFactor</url-pattern>
	</servlet-mapping>
</web-app>
